generator client {
  provider = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]

}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Review {
    id          String  @id @default(auto()) @map("_id") @db.ObjectId
    authorName  String
    position    String
    review      String
}
